<?xml version="1.0" encoding="utf-8"?>
<sprite version="1.0">
  <texture src="game/play/play.plist" tex="game/play/play.png"></texture>
  <mesh name="mesh_play_button_pause">
    <frame src="classic_button_pause.png"></frame>
  </mesh>
  <settings>
    <!--key name="deadpose" string="dismiss"></key-->
  </settings>
  <pose name="stand" collid="0.9,0.9">
    <part name="body">
      <mesh ref="mesh_play_button_pause" dt="1000"></mesh>
      <modifier mode="abc0">
        <key time="0"   yardscaley="4.8%"   alpha="100%"></key>
        <key time="200" yardscaley="5.8%"   alpha="100%"></key>
        <key time="400" yardscaley="6.8%"   alpha="100%"></key>
      </modifier>
    </part>
  </pose>
  <pose name="pressed" collid="0.9,0.9">
    <part name="body">
      <mesh ref="mesh_play_button_pause" dt="1000"></mesh>
      <modifier mode="abc0">
        <key time="000" yardscaley="6.8%" alpha="1.0"></key>
        <key time="100" yardscaley="9.9%" alpha="0.3"></key>
        <key time="130" yardscaley="6.8%" alpha="1.0"></key>
      </modifier>
    </part>
  </pose>
</sprite>
